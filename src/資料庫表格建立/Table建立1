CREATE TABLE EMP(
empNo NUMBER(10) Constraint EMP_EMPNO_Pk Primary Key,
empName VARCHAR2(30) NOT NULL,
empJob VARCHAR2(50) NOT NULL,
empId VARCHAR2(50) NOT NULL CONSTRAINT EMP_EMPID_UK UNIQUE,
empPwd VARCHAR2(1000) NOT NULL,
empPwdSalt VARCHAR2(1000) NOT NULL,
empStatus NUMBER(2) NOT NULL,
empHireDate Date NOT NULL,
empEmail VARCHAR2(100) NOT NULL
);

Create Sequence EMPNO_SQ  Increment By 1 Start With 6001 Nocache Nocycle;

