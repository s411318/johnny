CREATE TABLE DIARY (
 DIANO               		NUMBER(10) CONSTRAINT DIARY_DIANO_PK PRIMARY KEY,
 MEMNO                		NUMBER(10) NOT NULL constraint diary_mem_fk references member(memno),
 DIANAME                	VARCHAR2(100) NOT NULL,
 DIATEXT           		CLOB NOT NULL,
 DIAIMG                		BLOB,
 DIACRETIME             	TIMESTAMP NOT NULL,
 DIAMODTIME             	TIMESTAMP,
 DIASTATE			NUMBER(2) NOT NULL,
 DIAIMGEXTNAME			VARCHAR2(50) );
 
 CREATE SEQUENCE DIARY_SEQ
INCREMENT BY 1
START WITH 10001
NOMAXVALUE
NOCYCLE
NOCACHE;
 
